	"scripts": {
		"bootstrap": "pnpm install",
		"build:production": "cross-env NODE_OPTIONS=--max-old-space-size=4096 && vite build --mode production",
		"build:preview": "cross-env NODE_OPTIONS=--max-old-space-size=4096 && vite build --mode preview",
		"build:development": "cross-env NODE_OPTIONS=--max-old-space-size=8192 pnpm vite build --mode development",
		"build:stage": "cross-env NODE_OPTIONS=--max-old-space-size=8192 pnpm vite build --mode stage",
		"build:analyze": "cross-env NODE_OPTIONS=--max-old-space-size=8192 pnpm vite build --mode analyze",
		"build:no-cache": "pnpm store prune && npm run build",
		"commit": "czg",
		"dev": "pnpm vite",
		"preinstall": "npx only-allow pnpm",
		"postinstall": "turbo run stub",
		"lint": "turbo run lint",
		"lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
		"lint:prettier": "prettier --write .",
		"lint:stylelint": "stylelint \"**/*.{vue,css,less,scss}\" --fix --cache --cache-location node_modules/.cache/stylelint/",
		"prepare": "husky install",
		"reinstall": "rimraf pnpm-lock.yaml && rimraf package.lock.json && rimraf node_modules && npm run bootstrap",
		"serve": "vite dev --mode development",
		"serve:test": "vite dev --mode development",
		"serve:local": "vite dev --mode localhost",
		"serve:staging": "vite dev --mode stage",
		"serve:preview": "vite dev --mode preview",
		"test:gzip": "npx http-server dist --cors --gzip -c-1",
		"type:check": "vue-tsc --noEmit --skipLibCheck"
	},
	"lint-staged": {
		"*.{vue,js,ts}": [
			"eslint --fix"
		]
	},
